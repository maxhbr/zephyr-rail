/dts-v1/;

/ {
	#address-cells = < 0x1 >;
	#size-cells = < 0x1 >;
	model = "STMicroelectronics B-U585I-IOT02A discovery kit";
	compatible = "st,b-u585i-iot02a";
	chosen {
		zephyr,entropy = &rng;
		zephyr,flash-controller = &flash;
		zephyr,console = &usart1;
		zephyr,shell-uart = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
		zephyr,code-partition = &slot0_partition;
		zephyr,display = &ili9340;
		zephyr,keyboard-scan = &kscan_input;
	};
	aliases {
		watchdog0 = &iwdg;
		spi-flash0 = &mx25lm51245;
		die-temp0 = &die_temp;
		volt-sensor0 = &vref1;
		volt-sensor1 = &vbat4;
		led0 = &green_led_1;
		led1 = &red_led_1;
		sw0 = &user_button;
	};
	soc {
		#address-cells = < 0x1 >;
		#size-cells = < 0x1 >;
		compatible = "st,stm32u585", "st,stm32u5", "simple-bus";
		interrupt-parent = < &nvic >;
		ranges;
		nvic: interrupt-controller@e000e100 {
			#address-cells = < 0x1 >;
			compatible = "arm,v8m-nvic";
			reg = < 0xe000e100 0xc00 >;
			interrupt-controller;
			#interrupt-cells = < 0x2 >;
			arm,num-irq-priority-bits = < 0x4 >;
			phandle = < 0x1 >;
		};
		systick: timer@e000e010 {
			compatible = "arm,armv8m-systick";
			reg = < 0xe000e010 0x10 >;
		};
		flash: flash-controller@40022000 {
			compatible = "st,stm32-flash-controller", "st,stm32l5-flash-controller";
			reg = < 0x40022000 0x400 >;
			interrupts = < 0x6 0x0 >;
			#address-cells = < 0x1 >;
			#size-cells = < 0x1 >;
			flash0: flash@8000000 {
				compatible = "st,stm32-nv-flash", "soc-nv-flash";
				write-block-size = < 0x10 >;
				erase-block-size = < 0x2000 >;
				max-erase-time = < 0x5 >;
				reg = < 0x8000000 0x200000 >;
				partitions {
					compatible = "fixed-partitions";
					#address-cells = < 0x1 >;
					#size-cells = < 0x1 >;
					boot_partition: partition@0 {
						label = "mcuboot";
						reg = < 0x0 0x10000 >;
					};
					slot0_partition: partition@10000 {
						label = "image-0";
						reg = < 0x10000 0x68000 >;
					};
					slot1_partition: partition@78000 {
						label = "image-1";
						reg = < 0x78000 0x68000 >;
					};
					scratch_partition: partition@e0000 {
						label = "image-scratch";
						reg = < 0xe0000 0x10000 >;
					};
					storage_partition: partition@f0000 {
						label = "storage";
						reg = < 0xf0000 0x10000 >;
					};
				};
			};
		};
		rcc: rcc@46020c00 {
			compatible = "st,stm32u5-rcc";
			clocks-controller;
			#clock-cells = < 0x2 >;
			reg = < 0x46020c00 0x400 >;
			clocks = < &pll1 >;
			clock-frequency = < 0x9896800 >;
			ahb-prescaler = < 0x1 >;
			apb1-prescaler = < 0x1 >;
			apb2-prescaler = < 0x1 >;
			apb3-prescaler = < 0x1 >;
			phandle = < 0x3 >;
			rctl: reset-controller {
				compatible = "st,stm32-rcc-rctl";
				#reset-cells = < 0x1 >;
				phandle = < 0x4 >;
			};
		};
		exti: interrupt-controller@46022000 {
			compatible = "st,stm32g0-exti", "st,stm32-exti";
			interrupt-controller;
			#interrupt-cells = < 0x1 >;
			#address-cells = < 0x1 >;
			reg = < 0x46022000 0x400 >;
			num-lines = < 0x10 >;
			interrupts = < 0xb 0x0 >, < 0xc 0x0 >, < 0xd 0x0 >, < 0xe 0x0 >, < 0xf 0x0 >, < 0x10 0x0 >, < 0x11 0x0 >, < 0x12 0x0 >, < 0x13 0x0 >, < 0x14 0x0 >, < 0x15 0x0 >, < 0x16 0x0 >, < 0x17 0x0 >, < 0x18 0x0 >, < 0x19 0x0 >, < 0x1a 0x0 >;
			interrupt-names = "line0", "line1", "line2", "line3", "line4", "line5", "line6", "line7", "line8", "line9", "line10", "line11", "line12", "line13", "line14", "line15";
			line-ranges = < 0x0 0x1 >, < 0x1 0x1 >, < 0x2 0x1 >, < 0x3 0x1 >, < 0x4 0x1 >, < 0x5 0x1 >, < 0x6 0x1 >, < 0x7 0x1 >, < 0x8 0x1 >, < 0x9 0x1 >, < 0xa 0x1 >, < 0xb 0x1 >, < 0xc 0x1 >, < 0xd 0x1 >, < 0xe 0x1 >, < 0xf 0x1 >;
		};
		pinctrl: pin-controller@42020000 {
			compatible = "st,stm32-pinctrl";
			#address-cells = < 0x1 >;
			#size-cells = < 0x1 >;
			reg = < 0x42020000 0x2000 >;
			gpioa: gpio@42020000 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42020000 0x400 >;
				clocks = < &rcc 0x8c 0x1 >;
				phandle = < 0x2c >;
			};
			gpiob: gpio@42020400 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42020400 0x400 >;
				clocks = < &rcc 0x8c 0x2 >;
				phandle = < 0x2d >;
			};
			gpioc: gpio@42020800 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42020800 0x400 >;
				clocks = < &rcc 0x8c 0x4 >;
				phandle = < 0x2b >;
			};
			gpiod: gpio@42020c00 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42020c00 0x400 >;
				clocks = < &rcc 0x8c 0x8 >;
				phandle = < 0x2e >;
			};
			gpioe: gpio@42021000 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42021000 0x400 >;
				clocks = < &rcc 0x8c 0x10 >;
				phandle = < 0x2f >;
			};
			gpiof: gpio@42021400 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42021400 0x400 >;
				clocks = < &rcc 0x8c 0x20 >;
				phandle = < 0x30 >;
			};
			gpiog: gpio@42021800 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42021800 0x400 >;
				clocks = < &rcc 0x8c 0x40 >;
				phandle = < 0x32 >;
			};
			gpioh: gpio@42021c00 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42021c00 0x400 >;
				clocks = < &rcc 0x8c 0x80 >;
				phandle = < 0x31 >;
			};
			gpioi: gpio@42022000 {
				compatible = "st,stm32-gpio";
				gpio-controller;
				#gpio-cells = < 0x2 >;
				reg = < 0x42022000 0x400 >;
				clocks = < &rcc 0x8c 0x100 >;
			};
			adc1_in15_pb0: adc1_in15_pb0 {
				pinmux = < 0x210 >;
				phandle = < 0x20 >;
			};
			adc4_in19_pb1: adc4_in19_pb1 {
				pinmux = < 0x230 >;
				phandle = < 0x21 >;
			};
			dac1_out1_pa4: dac1_out1_pa4 {
				pinmux = < 0x90 >;
				phandle = < 0x1f >;
			};
			i2c1_scl_pb8: i2c1_scl_pb8 {
				pinmux = < 0x304 >;
				bias-pull-up;
				drive-open-drain;
				phandle = < 0xe >;
			};
			i2c2_scl_ph4: i2c2_scl_ph4 {
				pinmux = < 0xe84 >;
				bias-pull-up;
				drive-open-drain;
				phandle = < 0x10 >;
			};
			i2c1_sda_pb9: i2c1_sda_pb9 {
				pinmux = < 0x324 >;
				bias-pull-up;
				drive-open-drain;
				phandle = < 0xf >;
			};
			i2c2_sda_ph5: i2c2_sda_ph5 {
				pinmux = < 0xea4 >;
				bias-pull-up;
				drive-open-drain;
				phandle = < 0x11 >;
			};
			octospim_p2_io0_pf0: octospim_p2_io0_pf0 {
				pinmux = < 0xa05 >;
				slew-rate = "very-high-speed";
				phandle = < 0x16 >;
			};
			octospim_p2_io1_pf1: octospim_p2_io1_pf1 {
				pinmux = < 0xa25 >;
				slew-rate = "very-high-speed";
				phandle = < 0x17 >;
			};
			octospim_p2_io2_pf2: octospim_p2_io2_pf2 {
				pinmux = < 0xa45 >;
				slew-rate = "very-high-speed";
				phandle = < 0x18 >;
			};
			octospim_p2_io3_pf3: octospim_p2_io3_pf3 {
				pinmux = < 0xa65 >;
				slew-rate = "very-high-speed";
				phandle = < 0x19 >;
			};
			octospim_p2_clk_pf4: octospim_p2_clk_pf4 {
				pinmux = < 0xa85 >;
				slew-rate = "very-high-speed";
				phandle = < 0x14 >;
			};
			octospim_p2_dqs_pf12: octospim_p2_dqs_pf12 {
				pinmux = < 0xb85 >;
				slew-rate = "very-high-speed";
				phandle = < 0x1e >;
			};
			octospim_p2_io4_ph9: octospim_p2_io4_ph9 {
				pinmux = < 0xf25 >;
				slew-rate = "very-high-speed";
				phandle = < 0x1a >;
			};
			octospim_p2_io5_ph10: octospim_p2_io5_ph10 {
				pinmux = < 0xf45 >;
				slew-rate = "very-high-speed";
				phandle = < 0x1b >;
			};
			octospim_p2_io6_ph11: octospim_p2_io6_ph11 {
				pinmux = < 0xf65 >;
				slew-rate = "very-high-speed";
				phandle = < 0x1c >;
			};
			octospim_p2_io7_ph12: octospim_p2_io7_ph12 {
				pinmux = < 0xf85 >;
				slew-rate = "very-high-speed";
				phandle = < 0x1d >;
			};
			octospim_p2_ncs_pi5: octospim_p2_ncs_pi5 {
				pinmux = < 0x10a5 >;
				slew-rate = "very-high-speed";
				phandle = < 0x15 >;
			};
			spi1_miso_pe14: spi1_miso_pe14 {
				pinmux = < 0x9c5 >;
				bias-pull-down;
				phandle = < 0xb >;
			};
			spi1_mosi_pe15: spi1_mosi_pe15 {
				pinmux = < 0x9e5 >;
				bias-pull-down;
				phandle = < 0xc >;
			};
			spi1_nss_pe12: spi1_nss_pe12 {
				pinmux = < 0x985 >;
				bias-pull-up;
				phandle = < 0x9 >;
			};
			spi1_sck_pe13: spi1_sck_pe13 {
				pinmux = < 0x9a5 >;
				bias-pull-down;
				slew-rate = "very-high-speed";
				phandle = < 0xa >;
			};
			tim3_ch2_pe4: tim3_ch2_pe4 {
				pinmux = < 0x882 >;
				phandle = < 0x12 >;
			};
			tim4_ch1_pb6: tim4_ch1_pb6 {
				pinmux = < 0x2c2 >;
				phandle = < 0x13 >;
			};
			usart1_rx_pa10: usart1_rx_pa10 {
				pinmux = < 0x147 >;
				phandle = < 0x6 >;
			};
			usart3_rx_pd9: usart3_rx_pd9 {
				pinmux = < 0x727 >;
				phandle = < 0x8 >;
			};
			usart1_tx_pa9: usart1_tx_pa9 {
				pinmux = < 0x127 >;
				bias-pull-up;
				phandle = < 0x5 >;
			};
			usart3_tx_pd8: usart3_tx_pd8 {
				pinmux = < 0x707 >;
				bias-pull-up;
				phandle = < 0x7 >;
			};
			usb_otg_fs_dm_pa11: usb_otg_fs_dm_pa11 {
				pinmux = < 0x16a >;
				phandle = < 0x23 >;
			};
			usb_otg_fs_dp_pa12: usb_otg_fs_dp_pa12 {
				pinmux = < 0x18a >;
				phandle = < 0x24 >;
			};
		};
		iwdg: watchdog@40003000 {
			compatible = "st,stm32-watchdog";
			reg = < 0x40003000 0x400 >;
			status = "okay";
		};
		wwdg: wwdg1: watchdog@40002c00 {
			compatible = "st,stm32-window-watchdog";
			reg = < 0x40002c00 0x1000 >;
			clocks = < &rcc 0x9c 0x800 >;
			interrupts = < 0x0 0x7 >;
			status = "disabled";
		};
		backup_sram: memory@40036400 {
			compatible = "zephyr,memory-region", "st,stm32-backup-sram";
			reg = < 0x40036400 0x800 >;
			clocks = < &rcc 0x88 0x10020000 >;
			zephyr,memory-region = "BACKUP_SRAM";
			status = "disabled";
		};
		usart1: serial@40013800 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40013800 0x400 >;
			clocks = < &rcc 0xa4 0x4000 >;
			resets = < &rctl 0xf8e >;
			interrupts = < 0x3d 0x0 >;
			status = "okay";
			pinctrl-0 = < &usart1_tx_pa9 &usart1_rx_pa10 >;
			pinctrl-names = "default";
			current-speed = < 0x1c200 >;
		};
		usart2: serial@40004400 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40004400 0x400 >;
			clocks = < &rcc 0x9c 0x20000 >;
			resets = < &rctl 0xe91 >;
			interrupts = < 0x3e 0x0 >;
			status = "disabled";
		};
		usart3: arduino_serial: serial@40004800 {
			compatible = "st,stm32-usart", "st,stm32-uart";
			reg = < 0x40004800 0x400 >;
			clocks = < &rcc 0x9c 0x40000 >;
			resets = < &rctl 0xe92 >;
			interrupts = < 0x3f 0x0 >;
			status = "okay";
			pinctrl-0 = < &usart3_tx_pd8 &usart3_rx_pd9 >;
			pinctrl-names = "default";
			current-speed = < 0x1c200 >;
		};
		uart4: serial@40004c00 {
			compatible = "st,stm32-uart";
			reg = < 0x40004c00 0x400 >;
			clocks = < &rcc 0x9c 0x80000 >;
			resets = < &rctl 0xe93 >;
			interrupts = < 0x40 0x0 >;
			status = "disabled";
		};
		uart5: serial@40005000 {
			compatible = "st,stm32-uart";
			reg = < 0x40005000 0x400 >;
			clocks = < &rcc 0x9c 0x100000 >;
			resets = < &rctl 0xe94 >;
			interrupts = < 0x41 0x0 >;
			status = "disabled";
		};
		lpuart1: serial@46002400 {
			compatible = "st,stm32-lpuart", "st,stm32-uart";
			reg = < 0x46002400 0x400 >;
			clocks = < &rcc 0xa8 0x40 >;
			resets = < &rctl 0x1006 >;
			interrupts = < 0x42 0x0 >;
			status = "disabled";
		};
		spi1: arduino_spi: spi@40013000 {
			compatible = "st,stm32h7-spi", "st,stm32-spi-fifo", "st,stm32-spi";
			#address-cells = < 0x1 >;
			#size-cells = < 0x0 >;
			reg = < 0x40013000 0x400 >;
			interrupts = < 0x3b 0x5 >;
			clocks = < &rcc 0xa4 0x1000 >;
			status = "okay";
			pinctrl-0 = < &spi1_nss_pe12 &spi1_sck_pe13 &spi1_miso_pe14 &spi1_mosi_pe15 >;
			pinctrl-names = "default";
			cs-gpios = < &arduino_header 0x10 0x1 >, < &arduino_header 0xa 0x1 >;
			ili9340: ili9340@0 {
				compatible = "ilitek,ili9340";
				spi-max-frequency = < 0xe7319b >;
				reg = < 0x0 >;
				cmd-data-gpios = < &arduino_header 0xf 0x1 >;
				width = < 0x140 >;
				height = < 0xf0 >;
				pixel-format = < 0x1 >;
				rotation = < 0x5a >;
				frmctr1 = [ 00 18 ];
				pwctrl1 = [ 23 00 ];
				vmctrl1 = [ 3E 28 ];
				vmctrl2 = [ 86 ];
				pgamctrl = [ 0F 31 2B 0C 0E 08 4E F1 37 07 10 03 0E 09 00 ];
				ngamctrl = [ 00 0E 14 03 11 07 31 C1 48 08 0F 0C 31 36 0F ];
			};
			adafruit_2_8_tft_touch_v2_sdhc: sdhc@1 {
				compatible = "zephyr,sdhc-spi-slot";
				reg = < 0x1 >;
				status = "okay";
				spi-max-frequency = < 0x16e3600 >;
				mmc {
					compatible = "zephyr,sdmmc-disk";
					status = "okay";
				};
			};
		};
		spi2: spi@40003800 {
			compatible = "st,stm32h7-spi", "st,stm32-spi-fifo", "st,stm32-spi";
			#address-cells = < 0x1 >;
			#size-cells = < 0x0 >;
			reg = < 0x40003800 0x400 >;
