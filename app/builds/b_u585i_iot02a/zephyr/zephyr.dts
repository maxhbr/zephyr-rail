		};
		lptim1: timers@46004400 {
			compatible = "st,stm32-lptim";
			#address-cells = < 0x1 >;
			#size-cells = < 0x0 >;
			reg = < 0x46004400 0x400 >;
			clocks = < &rcc 0xa8 0x800 >, < &rcc 0xb 0x36ae8 >;
