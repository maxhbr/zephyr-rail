		};
	};
	gpio_keys {
		compatible = "gpio-keys";
		user_button: button {
			label = "User";
			gpios = < &gpioc 0xd 0x11 >;
		};
	};
	stepper: stepper {
		compatible = "maxhbr,stepper";
		gpios = < &gpiog 0x7 0x0 >, < &gpioe 0x4 0x0 >;
	};
};
