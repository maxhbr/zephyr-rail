/ {
	stepper_motor: stepper_motor {
		compatible = "simple-stepper";
		status = "okay";
		step-gpios = <&gpio0 29 GPIO_ACTIVE_HIGH>; /* needs to be inverted */
		dir-gpios = <&gpio0 4 GPIO_ACTIVE_HIGH>;
		en-gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
		micro-step-res = <16>;
	};
};